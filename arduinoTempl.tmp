{{define "arduinofile"}}
// Define any global variables here
// LED pins, etc.
String input = ""; // For collecting Serial input

void setup() {
  // This baud-rate is expected by the IOC
  Serial.begin(115200);
  // Write your setup code here
}

void loop() {
  while (Serial.available()>0) {
    char lastRecvd = Serial.read();
    if (lastRecvd == '\n') {
      switch (input[0]) { {{range .Write}}
      case '{{.SerialStr}}':
        // Write code here.
        break;{{end}}
{{range .Read}}
      case '{{.SerialStr}}':
        Serial.print("R ");
        // Write code here.
        // Make sure to Serial.print()
        // the expected data
        Serial.print("\n");
        input = "";
        break;
{{end}}
      default:
        break;
      }
    }
    else {
      input += lastRecvd;
    }
  }
}
{{end}}
